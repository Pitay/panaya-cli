{
  "name": "panaya-cli",
  "version": "1.0.0",
  "description": "panaya-cli - generate angular js file with ng-metadata",
  "main": "index.js",
  "dependencies": {
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "express": "^4.16.3",
    "figlet": "^1.2.0",
    "inquirer": "^6.1.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.7.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "core-js": "^2.4.1",
    "jquery": "^3.3.1",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "supertest": "^3.0.0",
    "typings": "^2.1.1"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json",
      "docs/*"
    ],
    "delay": "2500"
  },
  "scripts": {
    "start": "./node_modules/.bin/ts-node -- ./index.ts",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./index.ts",
    "tsc": "tsc index.js",
    "test": "nodemon --exec mocha -r ts-node/register src/**/*.test.ts src/**/tests/*.test.ts"
  },
  "author": "Itov",
